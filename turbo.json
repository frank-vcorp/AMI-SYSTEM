{
  "$schema": "https://turbo.build/json-schema.json",
  "globalDependencies": ["**/.env", "**/.env.local"],
  "pipeline": {
    "dev": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "build": {
      "outputs": ["dist/**", ".next/**", "out/**"],
      "dependsOn": ["^build"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": [],
      "dependsOn": []
    },
    "type-check": {
      "outputs": [],
      "dependsOn": ["^build"]
    }
  }
}
